<?php

/**
 * @file
 * Install, update and uninstall functions for esf_tc_advanced_search module.
 */

/**
 * Implements hook_install().
 */
function esf_tc_advanced_search_install() {
  $t = get_t();
  drupal_set_message($t("The feature ESF TC Advanced Search has been installed."));
}

/**
 * Implements hook_uninstall().
 */
function esf_tc_advanced_search_uninstall() {
  $t = get_t();
  drupal_set_message($t("The feature ESF TC Advanced Search has been uninstalled."));
}

/**
 * Implements hook_enable().
 */
function esf_tc_advanced_search_enable() {
  $t = get_t();
  drupal_set_message($t("The feature ESF TC Advanced Search has been enabled."));
}

/**
 * Implements hook_disable().
 */
function esf_tc_advanced_search_disable() {
  $t = get_t();
  drupal_set_message($t("The feature ESF TC Advanced Search has been disabled."));
}
